# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Database URL for Prisma (PostgreSQL)
# Obtén la contraseña de la base de datos en tu dashboard de Supabase
DATABASE_URL=

# Supabase Service Role Key (for server-side operations)
# Obtén esta clave en Settings > API de tu proyecto Supabase
SUPABASE_SERVICE_ROLE_KEY=

# Firebase Configuration
# Obtén estos valores en tu proyecto de Firebase Console
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=

# Google Gemini API Key (para extracción de menú con IA)
# Obtén tu API Key GRATIS en: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=

